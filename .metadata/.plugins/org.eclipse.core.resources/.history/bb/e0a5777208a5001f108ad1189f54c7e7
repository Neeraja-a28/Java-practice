package com.testdemo;

public class Test {
	public int a = 10;
	int b = 20;

	public static void main(String[] args) {
		System.out.println("hello world");
		Test t = new Test();
		t.method1();
		t.method2();

	}
	
	public void method1() {
		System.out.print("calling method1");
	}
	
	public int method2() {
		System.out.println(a + b);
		return a+b;
	}

}

